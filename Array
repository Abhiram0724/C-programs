#include<stdio.h>
int main()
{
  int arr[10],i,n;
  printf("Enter the number of elments:\n");
  scanf("%d",&n);
  printf("Enter the elements:\n");
  for(i=0;i<n;++i){
    scanf("%d",&arr[i]);
  }
    printf("The elements in the array are:\n");
  for(i=0;i<n;++i){
     printf("%d\n",arr[i]);
  }
return 0;

} 


