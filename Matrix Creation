#include<stdio.h>
int main()
{
  int arr[10][10],i,j,m,n;
  printf("Enter no. of rows and coloumns: \n");
  scanf("%d%d",&m,&n);
  printf("Enter the elements:\n");
  for(i=0;i<m;++i)
  {
    for(j=0;j<n;++j)
      scanf("%d",&arr[i][j]);
  }
  printf("The matrix is: \n");
  for(i=0;i<m;++i)
  {
    for(j=0;j<n;++j)
      printf("\t%d",arr[i][j]);
  
   printf("\n\n");
  } 
  
  return 0;
}   
