#include<stdio.h>
int main()
{
  int arr[10],i,n,j,t;
  printf("Enter the number of elments:\n");
  scanf("%d",&n);
  printf("Enter the elements:\n");
  for(i=0;i<n;++i){
    scanf("%d",&arr[i]);
  }
  printf("Reversed array is:\n");
  for(i=n-1;i>=0;--i)
   {printf("%d\n",arr[i]);}

  return 0;
}  
