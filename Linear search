 #include<stdio.h>
int main()
{
  int arr[10],i,n,x,idx=1;
  printf("Enter the number of elments:\n");
  scanf("%d",&n);
  printf("Enter the elements:\n");
  for(i=0;i<n;++i){
    scanf("%d",&arr[i]);
  }
  printf("Enter the element to be searched:\n");
  scanf("%d",&x);
  for(i=0;i<n;++i)
   {
     if(arr[i]==x)
      {
        ++idx;
        break;

      } 
      
   }
  if(idx==1)
    printf("Element not found\n");
  else
    printf("Element found\n");

  return 0;    
}
