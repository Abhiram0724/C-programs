#include<stdio.h>
int main()
{ 
  char ch;
  printf("Enter a character: \n");
  scanf("%c",&ch);
  if((ch>='a'&ch<='z')||(ch>='A'&ch<='Z'))
    printf("The character is alphabet\n");
  else if(ch>='0'&ch<='9')
    printf("The character is digit\n");
  else 
     printf("The character is special case\n");

  return 0;       
    
}
